{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": "src",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "checkJs": true,
    "paths": {
      "@/src/*": ["./*"],
      "@/lib/*": ["./lib/*"],
      "@/util/*": ["./lib/util/*"],
      "@/data/*": ["./data*"],
    }
  },
}
